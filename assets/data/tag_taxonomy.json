  "semanticRules": [
    {
      "category": "Artists",
      "priority": 6,
      "keywords": ["style", "artstyle", "illustration", "digital_painting", "painterly", "studio", "portfolio"],
      "prefixes": ["artist:", "style_of_", "art_of_", "by_", "inspired_by_"],
      "suffixes": ["_style", "_art", "_inspired"]
    },
    {
      "category": "Quality",
      "priority": 5,
      "keywords": ["masterpiece", "best_quality", "ultra_detailed", "incredibly_detailed", "8k", "4k", "absurdres", "hdr", "cinematic_lighting"],
      "suffixes": ["_quality", "_render"]
    },
    {
      "category": "Emotion & Expression",
      "priority": 4,
      "keywords": ["smile", "laugh", "cry", "blush", "angry", "frown", "wink", "tears", "grin", "surprised"],
      "suffixes": ["_smile", "_expression"],
      "patterns": ["(?<!no_)expression$"]
    },
    {
      "category": "Interaction",
      "priority": 3,
      "keywords": ["hug", "kissing", "kiss", "holding_hands", "handholding", "duo", "group", "leaning", "embrace", "sparring"],
      "suffixes": ["_together", "_pair"],
      "patterns": ["\\bwith_", "\\band_"]
    },
    {
      "category": "Species & Race",
      "priority": 4,
      "keywords": ["wolf", "fox", "dragon", "cat", "dog", "bunny", "rabbit", "lion", "tiger", "horse", "bird", "avian", "shark", "otter", "bear", "kobold", "lizard", "reptile", "monster", "demon", "angel", "elf", "orc", "succubus", "mermaid"],
      "suffixes": ["_folk", "_people", "_tribe"],
      "patterns": ["(?<!human)_ears$", "(?<!human)_tail$"]
    },
    {
      "category": "Copyright & Franchise",
      "priority": 3,
      "keywords": ["series", "franchise", "copyright"],
      "patterns": ["\\(.+\\)$"],
      "suffixes": ["_series", "_franchise"]
    },
    {
      "category": "Lighting & Effects",
      "priority": 3,
      "keywords": ["lighting", "rim_light", "backlight", "glow", "bokeh", "sparkle", "particles", "ray", "volumetric"],
      "suffixes": ["_lighting", "_glow", "_effect"],
      "patterns": ["light_beams?", "god_rays"]
    },
    {
      "category": "Setting & Environment",
      "priority": 3,
      "keywords": ["background", "indoors", "outdoors", "cafe", "forest", "city", "skyline", "mountain", "beach", "garden"],
      "suffixes": ["_background", "_scenery"],
      "patterns": ["\\broom$", "\\bstreet$", "\\bshop$"]
    },
    {
      "category": "Attire",
      "priority": 3,
      "keywords": ["dress", "uniform", "armor", "outfit", "jacket", "coat", "kimono", "suit", "robe", "hoodie", "bikini", "swimsuit", "lingerie"],
      "suffixes": ["_uniform", "_wear", "_outfit", "_armor"],
      "patterns": ["\\bskirt$", "\\bclothing$"]
    },
    {
      "category": "Accessories",
      "priority": 2,
      "keywords": ["hat", "glasses", "earrings", "necklace", "bracelet", "ribbon", "bow", "scarf", "brooch"],
      "suffixes": ["_hat", "_bow", "_scarf", "_accessory"],
      "patterns": ["\\bring$", "hair_\\w+"]
    }
  ],
